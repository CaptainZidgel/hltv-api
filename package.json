{
  "name": "hltv-api",
  "version": "0.0.0-development",
  "description": "API from Hltv.org",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcom.info | codecov",
    "test": "./node_modules/.bin/_mocha src/index.test.js --compilers js:babel-register",
    "test:w": "npm t -- -w",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dajk/hltv-api.git"
  },
  "keywords": [
    "Hltv.org",
    "API"
  ],
  "author": "Radovan Hajdukovic <hajdukovic.radovan@gmail.com> (https://at.linkedin.com/in/radovan-hajdukovic-a2004984)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dajk/hltv-api/issues"
  },
  "homepage": "https://github.com/dajk/hltv-api#readme",
  "dependencies": {
    "request": "^2.81.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "codecov": "^2.1.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^3.19.0",
    "mocha": "^3.2.0",
    "nyc": "^10.2.0",
    "rimraf": "^2.6.1",
    "semantic-release": "^6.3.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
}
